namespace cruise.umple.compiler;
use SimpleMetrics_Util.ump;
 
class StateMachineMetrics{  
    depend java.text.*;
  depend java.util.*;
  isA SimpleMetricsGenerator;
  StringBuilder code = null;
  UmpleModel model=null;

  void calculate(){
      // Output basic file header
      
       code.append("<h1>State Machine (SM) Measures</h1>");
    
      // Preparing Head (columns) of output file
      List<String>title = new ArrayList<String>();
      title.add(" Class Name                   ");
      title.add(" #State Machines ");
      title.add(" #States ");
      title.add(" #Transitions ");
      title.add(" #Different Transitions ");
      title.add(" #Events ");
      title.add(" #Guards ");
      title.add(" #Actions ");
      title.add(" #Nested SM ");
      title.add(" #Cycles ");
      title.add(" #Self Cycles ");
    
      code.append("<table border=\"1\"><tbody>\n");    
    
      code.append("<tr>");
      for (int i=0;i<title.size();i++){
          _genCell(0,code,title.get(i));
      }
      code.append("</tr>");

    DecimalFormat df = new DecimalFormat( "###,##0.0###");

      CountSM countPerClass = new CountSM(); // Class that contains values per class of counting about State Machine Metrics
    CountSM countTotals = new CountSM();   // Class that contains Max, Min and totals values of counting about State Machine Metrics
    boolean FirstTime = true; // used to allow setting first counted values to Min and Max attributes
      for (UmpleClass uClass : model.getUmpleClasses()) {  
      
    if (uClass!=null){

      code.append("<tr>");
      genCellString(code, uClass.getName());

      // CALCULATING STATE MACHINE METRICS
      countPerClass.calculate(uClass);
      
      // writing and formating string to record in output file
      genCellInt(code,countPerClass.getTotalStateMachinesPerClass());
      genCellInt(code,countPerClass.getTotalStatesPerClass());
      genCellInt(code,countPerClass.getTotalTransitionsPerClass());
      genCellInt(code,countPerClass.getTotalDifferentTransitionsPerClass());
      genCellInt(code,countPerClass.getTotalEventsPerClass());
      genCellInt(code,countPerClass.getTotalGuardsPerClass());
      genCellInt(code,countPerClass.getTotalActionsPerClass());
      genCellInt(code,countPerClass.getTotalNestedStateMachinePerClass());
      genCellInt(code,countPerClass.getTotalCyclesPerClass());
      genCellInt(code,countPerClass.getTotalSelfCyclesPerClass());

      
      // Sum all values per class in Totals
      countTotals.incTotal(countPerClass); 
      
      // if it is first loop, the first calculated values must be set like Max and Min values
      if (FirstTime){
        countTotals.setMinMax(countPerClass);
      FirstTime= false;
      };
      countTotals.calcMinMax(countPerClass); 
          countPerClass = new CountSM(); // Class that keep values of counting about State Machine Metrics
        }
        
       code.append("</tr>");        
      }

    // printing totals
      code.append("<tr>");    
      genCellString(code,"TOTAL");
      genCellInt(code,countTotals.getTotalStateMachines());
      genCellInt(code,countTotals.getTotalStates());
      genCellInt(code,countTotals.getTotalTransitions());
      genCellInt(code,countTotals.getTotalDifferentTransitions());
      genCellInt(code,countTotals.getTotalEvents());
      genCellInt(code,countTotals.getTotalGuards());
      genCellInt(code,countTotals.getTotalActions());
      genCellInt(code,countTotals.getTotalNestedStateMachine());
      genCellInt(code,countTotals.getTotalCycles());
      genCellInt(code,countTotals.getTotalSelfCycles());
      code.append("</tr>");
      
    // printing averages
    int tc = countTotals.getTotalClass();
    String res;
      code.append("<tr>");    
      genCellString(code,"AVERAGE");
      genCellFloat(code,(float)countTotals.getTotalStateMachines()/tc);
      genCellFloat(code,(float)countTotals.getTotalStates()/tc);
      genCellFloat(code,(float)countTotals.getTotalTransitions()/tc);
      genCellFloat(code,(float)countTotals.getTotalDifferentTransitions()/tc);
      genCellFloat(code,(float)countTotals.getTotalEvents()/tc);
      genCellFloat(code,(float)countTotals.getTotalGuards()/tc);
      genCellFloat(code,(float)countTotals.getTotalActions()/tc);
      genCellFloat(code,(float)countTotals.getTotalNestedStateMachine()/tc);
      genCellFloat(code,(float)countTotals.getTotalCycles()/tc);
      genCellFloat(code, (float)countTotals.getTotalSelfCycles()/tc);
      code.append("</tr>");

    
    // printing MAX values
      code.append("<tr>");    
      genCellString(code,"MAX");
      genCellInt(code,countTotals.getTotalStateMachinesPerClassMax());
      genCellInt(code,countTotals.getTotalStatesPerClassMax());
      genCellInt(code,countTotals.getTotalTransitionsPerClassMax());
      genCellInt(code,countTotals.getTotalDifferentTransitionsPerClassMax());
      genCellInt(code,countTotals.getTotalEventsPerClassMax());
      genCellInt(code,countTotals.getTotalGuardsPerClassMax());
      genCellInt(code,countTotals.getTotalActionsPerClassMax());
      genCellInt(code,countTotals.getTotalNestedStateMachinePerClassMax());
