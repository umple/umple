      genCellInt(code,countTotals.getTotalEventsPerClassMin());
      genCellInt(code,countTotals.getTotalGuardsPerClassMin());
      genCellInt(code,countTotals.getTotalActionsPerClassMin());
      genCellInt(code,countTotals.getTotalNestedStateMachinePerClassMin());
