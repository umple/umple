				   } else if (getModel().getUmpleInterface(str)!=null) {
					   
				   } else {
					   getParseResult().addErrorMessage( new ErrorMessage( 223, uTrait.getPosition(0), str, gtParameter.getName(), uTrait.getName()) ); 
