class Student {
  String foo(int a) Java {
    if(a < 0) {
      return 0;
    }

    for(int i = 0; i < a; i++) {
      if(i == a/4) {
        return a;
      }
    }

    return 4;
  }

  before custom foo Java { System.out.println("Starting foo with argument: " + a);  }

  after all foo Java { System.out.println("Returning from foo!");  }
}
