namespace example;

class Student
{
  Integer intId;
  Double doubleId;
  Boolean booleanId;
  String stringId;
  String[] stringListIds;
  
  key { intId, doubleId, booleanId, stringId, stringListIds }
}


class Mentor
{
  0..1 mentorMain -- 0..1 Course main;
  0..1 mentorSecondary -- * Course secondaries;
  
  key { main, secondaries }
}

class Course
{
  lazy immutable id;
  immutable name;
  section;
  key {}
}