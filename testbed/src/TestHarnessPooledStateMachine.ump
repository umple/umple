class PooledSM_ConcurrentState
{
  pooled sm
  {
    s1
    {
      e1 -> s2;
    }
    s2
    {
      e2 -> s1;
      s2a
      {
        e3 -> s2b;
        e5 -> s1;
      }
      s2b
      {
        e3 -> s2a;
      }
      
      ||
      
      s2c
      {
        e4 -> s2d;  
      }
      
      s2d
      {
        e4 -> s2c;
      }     
      
    }
  
  }
}

