class PooledSM_timedTransition_1 {
  String log = "initial value";
  pooled sm {
    s0 {
      entry / {log = "running";}
      ev0 -> s1;
    }
    s1 {
      after(2) -> s2;
    }
    s2 {
      ev2 -> s4;
    }
    s3 {
      ev3 -> s4;
    }
    s4 {}
  }
}


