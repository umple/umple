  {
    Open {
      register -> Full;
    }
    Full {
      reject -> Full;
    }
  }
}

class LightFixture
{
  Integer brightness = 0;
  Integer dimmer;
  pooled bulb{
	Off{
	    turnDimmer(Integer lightval) /{setBrightness(lightval);} -> Off;
	    flipSwitch -> Dimmed;
	}
	Dimmed{
		entry[dimmer > 99] -> On;
		flipSwitch -> Off;
        turnDimmer(Integer lightval) /{setBrightness(lightval);} -> Dimmed;
	}
	On{
		flipSwitch -> Off;
        turnDimmer(Integer lightval) /{setBrightness(lightval);} -> Dimmed;
	}
  }
}

class Light
{
  pooled bulb
  {
