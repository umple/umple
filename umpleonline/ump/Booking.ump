// UML class diagram for a Booking on an airline, represented in Umple

class Booking {
  state {
    new { 
      assignSeat -> seatAssigned;
      cancel -> cancelled;
    }
    seatAssigned {
      cancel -> cancelled;
      checkIn -> checkedIn;
    }
    checkedIn {
      cancel -> cancelled;
      complete -> completed;
    }
    cancelled {}
    completed {}
  }
}
