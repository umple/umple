}

class AnnAttGroup
{
	String name;
}

class File
{
	isA MtcEntity;
}

class MtcApplication
{
	1..* application -- * CoverageFile coverageFileCov;
	1..* application -- * CriterionFile criterionFileCri;
}

class CoverageFile
{
	isA File;
	0..1 key -- * Record;
}

class CriterionFile
{
	isA File;
	1..* criterionFileC -- * Criterion criterionC;
}

class MtcEntity
{
	0..1 -> * File;
}

class HistoryEntry
{
	isA MtcEntity;
}

class Criterion
{
	isA MtcEntity;
	String ident;
	String name;
	String description;
}

class Instance
{
	isA MtcEntity;
	Integer index;
	String occ;
	* caller -- * Instance called;
	* related -- * Instance origin;
}

class AssociationClass
{
	1..* associationClassRs -- 1..* Result resultA;
	1..* associationClassRc -- * Record recordA;
}

class Result
{
	isA MtcEntity;
	Integer elementCount;
	* -- 1..* Record;
}

class Record
{
	isA MtcEntity;
	String creator;
