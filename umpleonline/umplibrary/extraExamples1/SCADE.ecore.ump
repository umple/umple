class AnnProperty
{
	String name;
	Integer type;
}

class AnnPropertyInt
{
	isA AnnProperty;
	Integer value;
}

class AnnPropertyBool
{
	isA AnnProperty;
	Boolean value;
}

class AnnPropertyChar
{
	isA AnnProperty;
	Char value;
}

class AnnPropertyReal
{
	isA AnnProperty;
	Double value;
