{
	String pathname;
	1..* key -- 1..* AnnCategory;
}

class AnnCategory
{
	String name;
	0..1 -> * Annotability anootability;
}

class Annotability
{
	Integer minCardinality;
	Integer maxCardinality;
	Boolean defaultNote;
