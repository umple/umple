/************
This file is an Umple translation of a model
of UIML version 3.0 found at the following URL
https://github.com/atlanmod/atlantic-zoo/blob/main/AtlantEcore/UIML-3.0.ecore

date : 2006/02/21

Domain : User interface (UI)

Description : The design objective of the User Interface Markup Language (UIML) is to provide a vendor neutral, canonical representation of any user interface (UI) suitable for mapping to existing languages. UIML provides a highly device-independent method to describe a user interface.

Title: User Interface Markup Language 

See : http://www.uiml.org/specs/uiml3/DraftSpec.htm

Authors : David Touzet
*************/
namespace UIML;

class UIML
{
	0..1 -> * Header head;
	0..1 -> * Template templates;
	0..1 -> * Interface interfaces;
	0..1 -> * Peer peers;
}

class Header
{
	0..1 -> * MetaData metas;
}

class MetaData
{
	String name;
	String content;
}

namespace Peer;

class Peer
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Presentation presentations;
	0..1 -> * Logic logics;
}

class Presentation
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String base;
	0..1 -> * Source;
	0..1 -> * DClass dClasses;
}

class Logic
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * DComponent dComponents;
}

class DComponent
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String mapsTo;
	String location;
	0..1 -> * Source;
	0..1 -> * DMethod dMethods;
}

class DClass
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String mapsTo;
	String mapsType;
	0..1 -> * Source;
	0..1 -> * DMethod dMethods;
	0..1 -> * DProperty dProperties;
	0..1 -> * Event events;
	0..1 -> * Listener listeners;
}

class DProperty
{
	String id;
	MapsTypeKind mapsType;
	String mapsTo;
	String returnType;
	0..1 -> * DMethod dMethods;
	0..1 -> * DParam dParams;
}

class DMethod
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String mapsTo;
	String returnType;
	0..1 -> * Source;
	0..1 -> * DParam dParams;
	0..1 -> * Script;
}

class DParam
{
	String id;
	String type;
	String value;
}

class Script
{
	isA ExtensibleElement;
	String id;
	String type;
	HowKind how;
	ExportKind export;
	String value;
	String source;
}

class Listener
{
	String class;
	String attacher;
}

namespace Interface;

class ExtensibleElement
{
}

class Source
{
}

class LocalSource
{
	isA Source;
	0..1 -> 1..* ExtensibleElement source;
}

class RemoteSource
{
	isA Source;
	String location;
}

class Interface
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Structure;
	0..1 -> * Style;
	0..1 -> * Content;
	0..1 -> * Behavior;
}

class Structure
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Part parts;
}

class Part
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String class;
	String wherePart;
	0..1 -> * Source;
	0..1 -> * Style;
	0..1 -> * Content;
	0..1 -> * Behavior;
	0..1 -> * Part parts;
	0..1 -> * Repeat repeats;
}

class Style
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Property properties;
}

class Property
{
	isA ExtensibleElement;
	String name;
	HowKind how;
	ExportKind export;
	String[] values;
	0..1 -> * Part parts;
	0..1 -> * Event events;
	0..1 -> * Source;
	0..1 -> * Constant constants;
	0..1 -> * Property properties;
	0..1 -> * Reference references;
	0..1 -> * Call calls;
	0..1 -> * Iterator iterators;
}

class Content
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Constant constants;
}

class Behavior
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> * Rule rules;
}

class Repeat
{
	0..1 -> 1..* Iterator;
	0..1 -> 1..* Part parts;
}

class Constant
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	String model;
	String value;
	0..1 -> * Source;
	0..1 -> * Constant constants;
}

class Reference
{
	0..1 -> * Constant;
}

class Rule
{
	isA ExtensibleElement;
	String id;
	HowKind how;
	ExportKind export;
	0..1 -> * Source;
	0..1 -> 1..* Condition;
	0..1 -> 1..* Action;
}

class Condition
{
}

class EventCondition
{
	isA Condition;
	0..1 -> 1..* Event;
}

class EqualCondition
{
	isA Condition;
	0..1 -> 1..* Equal;
}

class OpCondition
{
	isA Condition;
	0..1 -> 1..* Operation op;
}

class Action
{
}

class Action_1
{
	isA Action;
	0..1 -> * Event;
	0..1 -> * Property properties;
	0..1 -> * Call calls;
	0..1 -> * Restructure restructures;
}

class Action_2
{
	isA Action;
	0..1 -> * WhenTrue;
	0..1 -> * WhenFalse;
	0..1 -> * ByDefault;
}

class Call
{
	String name;
	0..1 -> * Parameter params;
}

class Parameter
{
	String name;
}

class PropertyParameter
{
	isA Parameter;
	0..1 -> 1..* Property;
}

class ReferenceParameter
{
	isA Parameter;
	0..1 -> 1..* Reference;
}

class CallParameter
{
	isA Parameter;
	0..1 -> 1..* Call;
}

class OperationParameter
{
	isA Parameter;
	0..1 -> 1..* Operation op;
}

class EventParameter
{
	isA Parameter;
	0..1 -> 1..* Event;
}

class ConstantParameter
{
	isA Parameter;
	0..1 -> 1..* Constant;
}

class IteratorParameter
{
	isA Parameter;
	0..1 -> 1..* Iterator;
}

class Iterator
{
	String id;
}

class ConstantIterator
{
	isA Iterator;
	0..1 -> 1..* Constant;
}

class PropertyIterator
{
	isA Iterator;
	0..1 -> 1..* Property;
}

class CallIterator
{
	isA Iterator;
	0..1 -> 1..* Call;
}

class TextIterator
{
	isA Iterator;
	Integer value;
}

class Event
{
	String class;
	0..1 -> * Part parts;
}

class Equal
{
	0..1 -> 1..* Event;
}

class EqualToConstant
{
	isA Equal;
	0..1 -> 1..* Constant;
}

class EqualToProperty
{
	isA Equal;
	0..1 -> 1..* Property;
}

class EqualToReference
{
	isA Equal;
	0..1 -> 1..* Reference;
}

class EqualToOperation
{
	isA Equal;
	0..1 -> 1..* Operation op;
}

class Operation
{
	String name;
	0..1 -> * Constant constants;
	0..1 -> * Property properties;
	0..1 -> * Reference references;
	0..1 -> * Call calls;
	0..1 -> * Operation ops;
	0..1 -> * Event events;
}

class Restructure
{
	isA ExtensibleElement;
	HowKind how;
	0..1 -> * Part atPart;
	0..1 -> * Part wherePart;
	0..1 -> * Source;
	0..1 -> * Template;
}

class Branch
{
	0..1 -> * Property properties;
	0..1 -> * Call calls;
	0..1 -> * Restructure;
	0..1 -> * Operation op;
	0..1 -> * Equal;
	0..1 -> * Event;
}

class WhenTrue
{
	isA Branch;
}

class WhenFalse
{
	isA Branch;
}

class ByDefault
{
	isA Branch;
}

namespace Template;

class Template
{
	String id;
}

class BehaviorTemplate
{
	isA Template;
	0..1 -> 1..* Behavior;
}

class DClassTemplate
{
	isA Template;
	0..1 -> 1..* DClass;
}

class DComponentTemplate
{
	isA Template;
	0..1 -> 1..* DComponent;
}

class ConstantTemplate
{
	isA Template;
	0..1 -> 1..* Constant;
}

class ContentTemplate
{
	isA Template;
	0..1 -> 1..* Content;
}

class InterfaceTemplate
{
	isA Template;
	0..1 -> 1..* Interface;
}

class LogicTemplate
{
	isA Template;
	0..1 -> 1..* Logic;
}

class PartTemplate
{
	isA Template;
	0..1 -> 1..* Part;
}

class PeerTemplate
{
	isA Template;
	0..1 -> 1..* Peer;
}

class PresentationTemplate
{
	isA Template;
	0..1 -> 1..* Presentation;
}

class PropertyTemplate
{
	isA Template;
	0..1 -> 1..* Property;
}

class RestructureTemplate
{
	isA Template;
	0..1 template -> 1..* Restructure;
}

class RuleTemplate
{
	isA Template;
	0..1 -> 1..* Rule;
}

class ScriptTemplate
{
	isA Template;
	0..1 -> 1..* Script;
}

class StructureTemplate
{
	isA Template;
	0..1 -> 1..* Structure;
}

class StyleTemplate
{
	isA Template;
	0..1 -> 1..* Style;
}