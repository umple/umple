	Boolean refModeR1C1;
	Boolean iteration;
	Integer maxIterations;
	Double maxChange;
	Boolean precisionAsDisplayed;
	Boolean doNotSaveLinkValues;
	Boolean noAutoRecover;
	Boolean acceptLabelsInFormulas;
	Boolean uncalced;
}

